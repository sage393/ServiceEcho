<?xml version="1.0" encoding="UTF-8"?>
<service name="Echo">
	<description>
		Web service que emite un saludo o una despedida
	</description>
	<messageReceivers>
		<messageReceiver
			mep="http://www.w3.org/ns/wsdl/in-out"
			class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
	</messageReceivers>
	<parameter name="ServiceClass">com.sam.autentica.ws.servidor.Echo
	</parameter>
	<operation name="saludar"
		mep="http://www.w3.org/ns/wsdl/in-out" />
	<operation name="despedir"
		mep="http://www.w3.org/ns/wsdl/in-out" />

</service>